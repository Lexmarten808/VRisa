{
  "expo": {
    "name": "VRISA Mobile",
    "slug": "vrisa-mobile",
    "version": "0.1.0",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "sdkVersion": "54.0.0",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "plugins": ["expo-router"],
    "ios": {"supportsTablet": true},
    "android": {"adaptiveIcon": {"backgroundColor": "#ffffff"}},
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
